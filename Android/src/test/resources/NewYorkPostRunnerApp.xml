<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="NYP test suite on Android Phone">

    <parameter name="OS" value="Android"/>
    <parameter name="appType" value="Phone"/>
    <parameter name="deviceType" value="Emulator"/>
    <parameter name="deviceName" value="Nexus"/>
    <parameter name="version" value="5.0.2"/>

    <test name="Test Main Menu" preserve-order="true">
        <classes>
            <class name="NYP.MainMenuTest"/>
        </classes>
    </test>

    <!-- Test1 specific params are defined below this-->
    <parameter name="appName" value="nyp.apk"/>
</suite>
